{
  "manifest_version": 3,
  "name": "Sunset Time Extension",
  "version": "1.0",
  "description": "A Chrome extension that tells the sunset time",

  "action": {
    "default_popup": "/src/popup.html"
  },
  "permissions": ["geolocation", "activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/bundle.js"]
    }
  ]
}
